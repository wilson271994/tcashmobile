import React, { Component } from 'react';
import {
  View,
  Text,
  StyleSheet,
  SafeAreaView,
  Image,
  TouchableOpacity,
  Dimensions,
} from 'react-native';

const { width } = Dimensions.get('window');

class TransferScreen extends Component {
  constructor(props) {
    super(props);
    this.state = {
      amount: '5,550',
    };
  }

  handleKeyPress = (key) => {
    this.setState((prevState) => {
      let newAmount = prevState.amount;
      if (key === 'x') {
        newAmount = newAmount.slice(0, -1);
      } else {
        newAmount += key;
      }
      return { amount: newAmount };
    });
  };

  renderKeypad() {
    const keys = [
      ['1', '2', '3'],
      ['4', '5', '6'],
      ['7', '8', '9'],
      ['.', '0', 'x'],
    ];

    return keys.map((row, rowIndex) => (
      <View key={rowIndex} style={styles.keypadRow}>
        {row.map((key) => (
          <TouchableOpacity
            key={key}
            style={styles.keypadButton}
            onPress={() => this.handleKeyPress(key)}
          >
            <Text style={styles.keypadButtonText}>{key}</Text>
          </TouchableOpacity>
        ))}
      </View>
    ));
  }

  render() {
    return (
      <SafeAreaView style={styles.safeArea}>
        {/* En-t√™te */}
        <View style={styles.header}>
          <TouchableOpacity>
            <Image
              source={require('./assets/icons/back_arrow.png')}
              style={styles.backIcon}
            />
          </TouchableOpacity>
          <TouchableOpacity style={styles.depositLimitButton}>
            <Text style={styles.depositLimitText}>limites de d√©p√¥t</Text>
          </TouchableOpacity>
        </View>

        {/* Zone de saisie du montant */}
        <View style={styles.inputSection}>
          <Image
            source={require('./assets/icons/loading_spinner.png')}
            style={styles.spinner}
          />
          <Text style={styles.instructionText}>Entrez le montant</Text>
          <Text style={styles.amountText}>{this.state.amount}</Text>
          <View style={styles.detailsSection}>
            <Text style={styles.detailText}>Fais T_cash:</Text>
            <Text style={styles.detailText}>Fais OM:</Text>
            <Text style={styles.detailText}>montant √† recevoir:</Text>
          </View>
        </View>

        {/* Carte de compte */}
        <View style={styles.paymentMethodCard}>
          <Image
            source={require('./assets/images/orange_money_logo.png')}
            style={styles.omLogo}
          />
          <View style={styles.accountInfo}>
            <Text style={styles.accountLabel}>l'argent sera d√©duit de:</Text>
            <Text style={styles.accountNumber}>+237 690 12 32 22</Text>
          </View>
          <TouchableOpacity style={styles.changeButton}>
            <Text style={styles.changeButtonText}>change</Text>
          </TouchableOpacity>
        </View>

        {/* Clavier num√©rique */}
        <View style={styles.keypadContainer}>
          {this.renderKeypad()}
          <TouchableOpacity style={styles.nextButton}>
            <Text style={styles.nextButtonText}>suivant</Text>
          </TouchableOpacity>
        </View>
      </SafeAreaView>
    );
  }
}

